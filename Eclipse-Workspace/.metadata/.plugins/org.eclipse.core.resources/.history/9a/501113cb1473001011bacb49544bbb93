import java.util.Scanner;

public class Exemplo02 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite seu nome completo:");
		String nome = sc.next();
		System.out.println("Seu nome Ã© "+ nome);
		sc.close();
	}

}
